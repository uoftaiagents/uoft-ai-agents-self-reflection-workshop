    "# 🧠 Step 2: The Self-Reflection Engine\n",
    "# This is where the magic happens - AI that improves its own thinking!\n",
    "\n",
    "def thinking_ai(question, show_process=True):\n",
    "    \"\"\"\n",
    "    Our main AI system that demonstrates self-reflection.\n",
    "    \n",
    "    This is the core algorithm:\n",
    "    1. Generate initial response (first draft)\n",
    "    2. Critique that response (find problems)\n",
    "    3. Generate improved response (incorporate feedback)\n",
    "    \n",
    "    Args:\n",
    "        question: The question to answer\n",
    "        show_process: Whether to show the thinking steps (great for learning!)\n",
    "    \n",
    "    Returns:\n",
    "        Dictionary with all three stages of thinking\n",
    "    \"\"\"\n",
    "    \n",
    "    if show_process:\n",
    "        print(f\"🎯 Question: {question}\")\n",
    "        print(\"\\n\" + \"=\"*80)\n",
    "    \n",
    "    # STAGE 1: Initial Thinking (First Draft)\n",
    "    if show_process:\n",
    "        print(\"💭 STAGE 1: Initial AI Response (First Draft)\")\n",
    "    \n",
    "    initial_prompt = f\"Please provide helpful advice for: {question}\"\n",
    "    first_response = smart_ai_simulator(initial_prompt)\n",
    "    \n",
    "    if show_process:\n",
    "        print(f\"First Answer: {first_response}\")\n",
    "        print(\"\\n\" + \"-\"*50)\n",
    "    \n",
    "    # STAGE 2: Self-Critique (Find Problems)\n",
    "    if show_process:\n",
    "        print(\"🔍 STAGE 2: AI Self-Critique (Finding Flaws)\")\n",
    "    \n",
    "    critique_prompt = f\"Critically review and find problems with this response: {first_response}\"\n",
    "    self_critique = smart_ai_simulator(critique_prompt, first_response)\n",
    "    \n",
    "    if show_process:\n",
    "        print(f\"Self-Critique: {self_critique}\")\n",
    "        print(\"\\n\" + \"-\"*50)\n",
    "    \n",
    "    # STAGE 3: Improvement (Better Version)\n",
    "    if show_process:\n",
    "        print(\"✨ STAGE 3: AI Self-Improvement (Better Version)\")\n",
    "    \n",
    "    improve_prompt = f\"Create an improved response addressing these issues: {self_critique}\"\n",
    "    improved_response = smart_ai_simulator(improve_prompt, self_critique)\n",
    "    \n",
    "    if show_process:\n",
    "        print(f\"Improved Answer: {improved_response}\")\n",
    "        print(\"\\n\" + \"=\"*80)\n",
    "        print(\"🎉 Self-reflection cycle complete!\")\n",
    "    \n",
    "    return {\n",
    "        'question': question,\n",
    "        'initial': first_response,\n",
    "        'critique': self_critique,\n",
    "        'improved': improved_response\n",
    "    }\n",
    "\n",
    "# Let's see our AI think for itself!\n",
    "print(\"🚀 Watching AI Improve Its Own Thinking:\")\n",
    "result = thinking_ai(\"How can I get better at competitive programming?\")"
   ]
