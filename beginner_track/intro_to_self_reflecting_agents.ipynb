    "    \n",
    "    improve_prompt = f\"Create an improved response addressing these issues: {self_critique}\"\n",
    "    improved_response = smart_ai_simulator(improve_prompt, self_critique)\n",
    "    \n",
    "    if show_process:\n",
    "        print(f\"Improved Answer: {improved_response}\")\n",
    "        print(\"\\n\" + \"=\"*80)\n",
