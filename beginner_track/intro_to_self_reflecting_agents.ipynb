   "source": [
    "# 🔥 Step 5: Advanced Multi-Round Reflection\n",
    "# This is where things get really interesting - recursive self-improvement!\n",
    "\n",
    "def deep_thinking_ai(question, max_rounds=3, quality_threshold=1.2):\n",
    "    \"\"\"\n",
    "    Advanced AI that keeps reflecting until it's satisfied with its answer.\n",
    "    \n",
    "    This demonstrates recursive self-improvement - a key concept in AI research!\n",
    "    \n",
    "    Args:\n",
    "        question: The question to deeply analyze\n",
    "        max_rounds: Maximum reflection cycles (prevents infinite loops)\n",
    "        quality_threshold: How much improvement we need to continue\n",
    "    \"\"\"\n",
    "    \n",
    "    print(f\"🧠 DEEP THINKING MODE: {question}\")\n",
    "    print(f\"🔄 Running up to {max_rounds} rounds of reflection...\")\n",
    "    print(\"\\n\" + \"=\"*80)\n",
    "    \n",
    "    # Start with initial response\n",
    "    current_response = smart_ai_simulator(f\"Provide a comprehensive answer to: {question}\")\n",
    "    thinking_history = []\n",
    "    \n",
    "    for round_num in range(1, max_rounds + 1):\n",
    "        print(f\"\\n🔄 ROUND {round_num} OF REFLECTION:\")\n",
    "        print(f\"Current answer length: {len(current_response)} characters\")\n",
    "        \n",
    "        # Critique current response\n",
    "        critique = smart_ai_simulator(f\"Critically analyze and find specific improvements for: {current_response}\")\n",
    "        print(f\"\\n🔍 Critique summary: {critique[:200]}...\")\n",
    "        \n",
    "        # Generate improved version\n",
    "        improved = smart_ai_simulator(f\"Create a significantly better response addressing: {critique}\")\n",
    "        print(f\"\\n✨ Improved answer length: {len(improved)} characters\")\n",
    "        \n",
    "        # Calculate improvement\n",
    "        improvement_factor = len(improved) / len(current_response) if len(current_response) else 0\n",
    "        print(f\"📈 Improvement factor: {improvement_factor:.2f}x\")\n",
    "        \n",
    "        # Store this round's thinking\n",
    "        thinking_history.append({\n",
    "            'round': round_num,\n",
    "            'response': current_response,\n",
    "            'critique': critique,\n",
    "            'improved': improved,\n",
    "            'improvement_factor': improvement_factor\n",
    "        })\n",
    "        \n",
    "        # Decide whether to continue\n",
    "        if improvement_factor >= quality_threshold:\n",
    "            current_response = improved\n",
    "            print(f\"➡️ Significant improvement detected ({improvement_factor:.2f}x), continuing...\")\n",
    "        else:\n",
    "            print(f\"✅ Converged! Improvement factor ({improvement_factor:.2f}x) below threshold ({quality_threshold}x)\")\n",
    "            break\n",
    "            \n",
    "        print(\"\\n\" + \"-\"*60)\n",
    "    \n",
    "    print(\"\\n\" + \"=\"*80)\n",
    "    print(\"🎯 FINAL EVOLVED ANSWER:\")\n",
    "    print(current_response)\n",
    "    \n",
    "    print(\"\\n📊 THINKING EVOLUTION SUMMARY:\")\n",
    "    for round_data in thinking_history:\n",
    "        print(f\"Round {round_data['round']}: {round_data['improvement_factor']:.2f}x improvement\")\n",
    "    \n",
    "    return {\n",
    "        'final_answer': current_response,\n",
    "        'thinking_history': thinking_history,\n",
    "        'total_rounds': len(thinking_history)\n",
    "    }\n",
    "\n",
    "# Let's watch AI think deeply about a complex question\n",
    "deep_result = deep_thinking_ai(\"What's the most effective path to becoming an expert software engineer?\")"
